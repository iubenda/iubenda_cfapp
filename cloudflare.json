{
    "main":"public/javascripts/iubenda_cf.js",
    "name":"iubenda",
    "category":"policy management",
    "description": "Every website needs a privacy policy. It's the law, everywhere. Iubenda lets you generate one in seconds, beautifully designed, customized on your needs and remotely maintained by a legal team.",
    "version": "1.0.3",
	"contributors":[
	{
		"name":"Domenico Vele",
		"email":"mimmo@iubenda.com"
	},{
		"name":"Andrea Giannangelo",
		"email":"andrea@iubenda.com"
	}
	],
    "account": {
        "callback_url": "https://www.iubenda.com/api/cloudflare",
        "user_fields": ["email"]
    },
    "billing": {
        "type": "zone",
        "plans":[
            {
                "name" : "iubenda Pro",
                "price": "2.25"
            }
        ]
    },
    "config":{
        "details":{
            "Price": "$2.25/month",
            "Category": "Privacy Policy Generator",
            "Restrictions": "None"
        },
        "assets":{
            "logos" : {
                "132px": "./public/images/logo-132.png",
                "200px": "./public/images/logo-200.png"
            },
            "detail_page" : [
                "./public/images/pp_screenshot_simple-1.png",
                "./public/images/pp_screenshot_full-1.png",
                "./public/images/headline-1.png"
            ]
        },
        "interface": [
            {
                "id": "skip_badge",
                "type": "select",
                "name": "Privacy policy widget",
                "description": "Automatically embed the privacy policy widget on every page",
                "options": [
                    {
                        "label": "Yes",
                        "value": false,
                        "selected": true
                    },
                    {
                        "label": "No",
                        "value": true
                    }
                ]
            }
        ]
    }
}
